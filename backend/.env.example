# Backend environment variables
# Clerk secret key for backend authentication
CLERK_SECRET_KEY=
# Optional if any backend logic needs the publishable key
CLERK_PUBLISHABLE_KEY=
# Postgres connection string
DATABASE_URL=
# Redis/Upstash URL for BullMQ jobs
UPSTASH_REDIS_URL=
# Optional standard Redis URL fallback
REDIS_URL=
# OneSignal backend credentials
ONESIGNAL_APP_ID=
ONESIGNAL_REST_API_KEY=
# Optional server port (defaults to 3000)
PORT=3000
