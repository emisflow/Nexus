# Backend (server + worker)
# Clerk server key (for Express middleware)
CLERK_SECRET_KEY=
# Optional if server needs publishable key for any SDK initialization
CLERK_PUBLISHABLE_KEY=
# Postgres connection string
DATABASE_URL=
# Redis for BullMQ/Upstash
UPSTASH_REDIS_URL=
# Optional fallback Redis URL
REDIS_URL=
# OneSignal backend keys
ONESIGNAL_APP_ID=
ONESIGNAL_REST_API_KEY=
# Optional server port (defaults to 3000)
PORT=3000

# Frontend (Next.js)
# Clerk publishable key for client
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
# Base URL for API requests
NEXT_PUBLIC_API_URL=
# OneSignal Web Push app id
NEXT_PUBLIC_ONESIGNAL_APP_ID=
